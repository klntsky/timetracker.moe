    "@typescript-eslint/parser": "^8.39.0",
    "@vitejs/plugin-react": "^4.2.0",
    "eslint": "^9.32.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^30.1.1",
    "jest-environment-jsdom": "^30.1.1",
    "prettier": "^3.6.2",
    "ts-jest": "^29.4.1",
    "typescript": "^5.5.0",
    "vite": "^5.2.0"
